#include <bits/stdc++.h>
using namespace std;


int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

  int n,k;
  int ans=0;
  cin>>n>>k;
  for(int i=0;i<n;i++){
        string a;
        cin>>a;
        int c=1;
        for(int j=0;j<=k;j++){
        if(a.find(char('0'+j))==-1)c=0;
      }
      ans+=c;
    }
  cout<<ans;
}
